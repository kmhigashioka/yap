import React from 'react';
import { Helmet } from 'react-helmet-async';
import { {{name}}Provider } from './{{name}}Context';

const {{name}} = (): React.ReactElement => (
  <{{name}}Provider>
    <Helmet>
      <title>{{name}}</title>
      <meta name="description" content="Description of {{name}}" />
    </Helmet>
    <div>{{name}}</div>
  </{{name}}Provider>
);

export default {{name}};
